#!/bin/bash

source /usr/local/pf/docker/systemd-service

name=httpd.portal

args=`base_args $name`
args="$args -v/usr/local/pf/html/captive-portal/profile-templates:/usr/local/pf/html/captive-portal/profile-templates -v/usr/local/pf/conf/:/usr/local/pf/conf/ -v/usr/local/pf/var/conf/:/usr/local/pf/var/conf/ -p 127.0.0.1:80:80"

run $name "$args"
