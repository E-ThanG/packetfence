#!/bin/bash

source /usr/local/pf/docker/systemd-service

name=haproxy-portal

args=`base_args $name`
args="$args -v/usr/local/pf/conf/ssl/:/usr/local/pf/conf/ssl/ -v/usr/local/pf/var/conf/:/usr/local/pf/var/conf/ -v/usr/local/pf/html/captive-portal/templates:/usr/local/pf/html/captive-portal/templates -p 80:80 -p 443:443"

run $name "$args"
